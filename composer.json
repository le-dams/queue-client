  "type": "library",
  "keywords": [
    "queue",
    "client"
  ],
  "authors": [{
    "name":     "Damien Bourcy",
    "email":    "info@damien-bourcy.com",
    "homepage": "http://www.damien-bourcy.com/"
  }],
  "license": "BSD-3-Clause",
